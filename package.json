{
  "name": "react-starter",
  "version": "0.1.0",  
  "scripts": {
    "dev": "next dev",
    "dev-open": "start http://localhost:3000 && pnpm dev",
    "build": "next build",
    "build-analyze": "cross-env ANALYZE=true pnpm build",
    "oldExport": "rimraf ./out && rimraf ./.next && cross-env NODE_ENV=production next build && next export",
    "export": "next export",
    "deploy": "next build && next export && touch out/.nojekyll && gh-pages -d out",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:unit && pnpm test:component && pnpm test:e2e",
    "test:unit": "jest",
    "test:e2e-open": "pnpm test:e2e --ui",
    "test:component-open": "pnpm test:component --ui",
    "feature:remove": "node ./helpers/featureSelect.ts"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@fontsource/roboto": "^5.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "@mui/material-nextjs": "^6.1.3",
    "axios": "1.6.5",
    "clsx": "1.2.1",
    "colorette": "2.0.19",
    "mobx": "6.12.0",
    "mobx-react-lite": "3.4.3",
    "next": "14.0.3",
    "next-react-svg": "1.2.0",
    "noty": "3.2.0-beta-deprecated",
    "pixelmatch": "5.3.0",
    "pngjs": "7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.69.7",
    "typescript": "5.0.2",
    "webpack": "5.89.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.0.3",
    "@svgr/webpack": "8.1.0",
    "@total-typescript/ts-reset": "0.5.1",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "eslint-config-next": "14.0.3",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "fs-extra": "11.2.0",
    "kill-port": "2.0.1",
    "postcss": "8.4.33",
    "puppeteer": "19.11.1",
    "rimraf": "4.4.1",
    "sass-loader": "13.3.3",
    "webpack-dev-server": "4.13.1",
    "gh-pages": "^3.2.3"
  }
}
